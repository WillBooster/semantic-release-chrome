{
  "name": "semantic-release-chrome",
  "version": "0.0.0-semantically-released",
  "description": "A semantic-release plugin for publishing a Chrome extension",
  "keywords": [
    "semantic-release",
    "plugin",
    "chrome",
    "webstore",
    "extension",
    "publish",
    "version"
  ],
  "homepage": "https://github.com/WillBooster/semantic-release-chrome#readme",
  "bugs": {
    "url": "https://github.com/WillBooster/semantic-release-chrome/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WillBooster/semantic-release-chrome.git"
  },
  "license": "MIT",
  "author": "WillBooster Inc.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "gd-scripts start",
    "build": "gd-scripts build",
    "ci": "gd-scripts ci",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit --Pretty",
    "release": "semantic-release",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --color --write \"**/{.*/,}*.{css,htm,html,js,json,json5,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\""
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@semantic-release/error": "2.2.0",
    "aggregate-error": "3.0.0",
    "archiver": "3.0.0",
    "chrome-webstore-upload": "0.2.2",
    "fs-extra": "7.0.1"
  },
  "devDependencies": {
    "@types/adm-zip": "0.4.32",
    "@types/archiver": "2.1.3",
    "@types/fs-extra": "5.0.5",
    "@types/node": "11.13.9",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "@willbooster/eslint-config-ts": "6.2.0",
    "@willbooster/prettier-config": "6.0.0",
    "@willbooster/renovate-config": "6.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "gd-scripts": "5.0.3",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "micromatch": "4.0.2",
    "prettier": "2.2.1",
    "semantic-release": "15.13.3",
    "sort-package-json": "1.49.0",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">=8.3"
  }
}
